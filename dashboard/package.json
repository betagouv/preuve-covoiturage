{
  "name": "dashboard",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "precommit": "run-s format:fix lint",
    "build:review": "eval \"echo \\\"$(cat src/environments/environment.review.ts.template)\\\"\" | sed 's|$REVIEW_ID|'$REVIEW_ID'|g' | sed 's|$BRANCH|'$BRANCH'|g' > src/environments/environment.review.ts && cp -f src/environments/environment.review.ts src/environments/environment.ts && ng build --configuration=review",
    "build": "ng build --configuration=$NODE_ENV",
    "lint": "ng lint",
    "format:fix": "pretty-quick --staged",
    "e2e": "yarn run cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/material": "^8.0.2",
    "@angular/material-moment-adapter": "^8.1.3",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@ctrl/ngx-chartjs": "^2.0.0",
    "@types/leaflet": "^1.5.1",
    "@types/leaflet.markercluster": "^1.4.0",
    "chart.js": "^2.8.0",
    "hammerjs": "^2.0.8",
    "leaflet": "^1.5.1",
    "leaflet.markercluster": "^1.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "ng2-nouislider": "^1.8.2",
    "ngx-infinite-scroll": "^8.0.0",
    "ngx-toastr": "^10.0.4",
    "nouislider": "^14.0.2",
    "ol": "^5.3.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cypress": "^3.4.1",
    "husky": "2.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-import-group-ordering": "^2.1.1",
    "typescript": "~3.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run format:fix",
      "pre-push": "yarn run lint"
    }
  }
}
