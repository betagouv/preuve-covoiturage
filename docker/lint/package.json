{
  "name": "lint",
  "version": "1.0.0",
  "description": "docker-lint",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run api & npm run dashboard & npm run etl",
    "start:local": "npm run api:local & npm run dashboard:local & npm run etl:local",
    "api": "eslint --config .eslintrc.js --env node -f ./formatter.js '/code/api/**/src/**/*.ts'",
    "api:local": "eslint --config .eslintrc.js --env node '../../api/**/src/**/*.ts'",
    "dashboard": "eslint --config .eslintrc.js --env browser -f ./formatter.js '/code/dashboard/src/**/*.ts'",
    "dashboard:local": "eslint --config .eslintrc.js --env browser '../../dashboard/src/**/*.ts'",
    "etl": "eslint --config .eslintrc.js --env browser -f ./formatter.js '/code/etl/src/**/*.ts'",
    "etl:local": "eslint --config .eslintrc.js --env browser '../../etl/src/**/*.ts'",
    "clean": "rm -rf node_modules/*"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
