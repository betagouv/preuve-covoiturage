{
  "extends": "nx/presets/npm.json",
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "test:unit": {
      "cache": true,
      "dependsOn": ["^build", "build"]
    },
    "coverage": {
      "cache": true
    }
  },
  "affected": {
    "defaultBase": "main"
  },
  "parallel": 16
}
