{
  "name": "@pdc/api",
  "private": true,
  "engines": {
    "node": "20"
  },
  "repository": "git@github.com:betagouv/preuve-covoiturage.git",
  "author": "Registre de preuve de covoiturage <contact@covoiturage.beta.gouv.fr>",
  "contributors": [
    "Jonathan Fallon <jonathan.fallon@beta.gouv.fr>",
    "Nicolas MÃ©rigot <nicolas.merigot@beta.gouv.fr>",
    "Julien Parmentier <julien.parmentier@beta.gouv.fr>"
  ],
  "scripts": {
    "dev": "npm run start:http 8080 | pino-pretty -f",
    "start": "if [ \"$DO_MIGRATIONS\" ]; then npm run migrate; fi && if [ -z \"$IS_WORKER\" ]; then npm run start:http; else npm run start:queue; fi",
    "start:http": "node dist/main.js http $PORT",
    "start:queue": "node dist/main.js queue $APP_REDIS_URL",
    "clean": "rm -rf ./dist",
    "copy-static": "mkdir -p dist/ilos/cli dist/pdc/providers/seed dist/pdc/proxy; cp -R ./src/ilos/cli/stubs ./dist/ilos/cli; cp -R ./src/pdc/providers/seed/*.csv ./dist/pdc/providers/seed; cp -R ./src/pdc/proxy/public ./dist/pdc/proxy",
    "build": "npm run copy-static && tsc",
    "watch": "tsc -w",
    "test:unit": "ava --config ava.unit.cjs",
    "test:integration": "ava --config ava.integration.cjs",
    "coverage:ci": "nyc --nycrc-path ./nyc.config.cjs --reporter=lcov ava --config ava.coverage.cjs",
    "coverage": "nyc --nycrc-path ./nyc.config.cjs --reporter=text ava --config ava.coverage.cjs",
    "audit:deps": "npm audit --audit-level moderate",
    "ilos": "node dist/main.js",
    "migrate": "DATABASE_URL=$APP_POSTGRES_URL POSTGRES_SCHEMA=geo node src/db/cli.js"
  },
  "keywords": [
    "betagouv",
    "covoiturage",
    "api",
    "carpooling"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/betagouv/preuve-covoiturage/issues"
  },
  "homepage": "https://github.com/betagouv/preuve-covoiturage#readme",
  "_moduleAliases": {
    "@pdc": "dist/pdc",
    "@ilos": "dist/ilos",
    "@config": "dist/config",
    "@shared": "dist/shared",
    "@db": "src/db"
  },
