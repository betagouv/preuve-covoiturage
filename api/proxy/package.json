{
  "name": "@pdc/proxy",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "ilos": "ilos",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "NODE_ENV=testing mocha --exit src/*.spec.ts src/**/*.spec.ts",
    "test:integration": "NODE_ENV=testing mocha --exit tests/*.spec.ts",
    "coverage-ci": "NODE_ENV=testing nyc --report-dir=../coverage/proxy --temp-dir=../.nyc_output --all --reporter=lcov mocha --exit src/*.spec.ts src/**/*.spec.ts",
    "coverage": "NODE_ENV=testing nyc --all --reporter=text mocha --exit src/*.spec.ts src/**/*.spec.ts",
    "lint": "tslint -c ../tslint.json -p tsconfig.json",
    "ingest": "mocha --exit tests/ingest.spec.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "config": {
    "workingDir": "./dist",
    "bootstrap": "./bootstrap.js"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/helmet": "^0.0.44",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.6.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.0",
    "lodash": "^4.17.14",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.11",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-import-group-ordering": "^2.1.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@ilos/cli": "~0",
    "@ilos/common": "~0",
    "@ilos/config": "~0",
    "@ilos/connection-mongo": "~0",
    "@ilos/core": "~0",
    "@ilos/env": "~0",
    "@ilos/framework": "~0",
    "@ilos/notification": "~0",
    "@ilos/transport-http": "~0",
    "@ilos/transport-redis": "~0",
    "@pdc/provider-schema": "~0",
    "@pdc/provider-sentry": "~0",
    "@pdc/provider-token": "~0",
    "@pdc/service-acquisition": "~0",
    "@pdc/service-application": "~0",
    "@pdc/service-normalization": "~0",
    "@pdc/service-operator": "~0",
    "@pdc/service-territory": "~0",
    "@pdc/service-trip": "~0",
    "@pdc/service-user": "~0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-mung": "^0.5.1",
    "express-session": "^1.16.2",
    "helmet": "^3.19.0"
  }
}
