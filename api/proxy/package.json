    "@pdc/service-stats": "0.0.1",
    "@pdc/service-trip": "0.0.1",
    "@pdc/service-user": "0.0.1",
    "@pdc/shared": "0.0.1",
    "@sentry/node": "^5.1.0",
    "api-query-params": "^4.9.0",
    "axios": "^0.18.0",
