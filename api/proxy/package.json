{
  "name": "@pdc/proxy",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "ilos": "ilos",
    "watch": "tsc -w",
    "coverage-ci": "NODE_ENV=testing nyc --report-dir=../../coverage/proxy --temp-dir=../.nyc_output --all --reporter=lcov mocha src/*.spec.ts src/**/*.spec.ts",
    "coverage": "NODE_ENV=testing nyc --all --reporter=text mocha src/*.spec.ts src/**/*.spec.ts",
    "test": "export NODE_ENV=testing; mocha src/**/*.spec.ts",
    "test:integration": "export NODE_ENV=testing; mocha tests/**/*.spec.ts",
    "lint": "tslint -c ../tslint.json -p tsconfig.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "config": {
    "workingDir": "./dist",
    "bootstrap": "./bootstrap.js"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/helmet": "^0.0.43",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "@types/passport": "^1.0.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.11",
    "nyc": "^13.3.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-import-group-ordering": "^2.1.1",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@ilos/core": "~0",
    "@ilos/framework": "~0",
    "@ilos/cli": "~0",
    "@ilos/provider-env": "~0",
    "@ilos/provider-config": "~0",
    "@pdc/provider-sentry": "0.0.1",
    "@ilos/provider-notification": "~0",
    "body-parser": "^1.19.0",
    "bull-arena": "^2.6.2",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-mung": "^0.5.1",
    "express-session": "^1.16.1",
    "helmet": "^3.18.0",
    "swagger-ui-express": "^4.0.2"
  }
}
