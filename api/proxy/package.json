{
  "name": "@pdc/proxy",
  "version": "0.0.1",
  "description": "Proxy",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 ./app.js",
    "worker": "node ./worker.js",
    "worker-dev": "nodemon --inspect=0.0.0.0:9230 ./worker.js",
    "migrate": "npx migrate-mongo up",
    "mig:up": "cd ../.. && docker-compose exec api npx migrate-mongo up",
    "mig:down": "cd ../.. && docker-compose exec api npx migrate-mongo down",
    "mig:status": "cd ../.. && docker-compose exec api npx migrate-mongo status",
    "seed": "node cli/seed.js",
    "process": "yarn workspace @pdc/service-acquisition process",
    "process-trip": "yarn workspace @pdc/service-acquisition process-trip",
    "fix-permissions": "node cli/fix-permissions.js"
  },
  "dependencies": {
    "@pdc/package-email": "0.0.1",
    "@pdc/package-express-errors": "0.0.1",
