{
  "name": "@pdc/provider-token",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.js",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "coverage-ci": "NODE_ENV=testing nyc --report-dir=../../coverage/provider-token --temp-dir=../../.nyc_output --all --reporter=lcov mocha --exit src/*.spec.ts src/**/*.spec.ts",
    "coverage": "NODE_ENV=testing nyc --all --reporter=text mocha --exit src/*.spec.ts src/**/*.spec.ts",
    "lint": "eslint 'src/**/*.ts'",
    "test": "mocha --exit src/*.spec.ts src/**/*.spec.ts"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1"
  },
  "peerDependencies": {
    "@ilos/common": "~0",
    "@ilos/config": "~0",
    "@ilos/core": "~0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.11",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.5.3"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/interfaces/**",
      "src/types/**",
      "src/exceptions/**"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
