  "nyc": {
    "exclude": [
      "src/**/*.spec.ts",
      "src/**/interfaces/**",
      "src/**/exceptions/**",
      "src/config/**",
      "src/shared/**"
    ]
  },
  "dependencies": {
    "@ilos/cli": "~0",
    "@ilos/common": "~0",
    "@ilos/connection-manager": "~0",
    "@ilos/connection-postgres": "~0",
    "@ilos/connection-redis": "~0",
    "@ilos/core": "~0",
    "@pdc/helper-test": "~0",
    "@pdc/provider-middleware": "~0",
    "@pdc/provider-validator": "~0",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  }
}
