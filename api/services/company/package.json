{
  "name": "@pdc/service-company",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "cli": "ilos",
    "migrate": "ilos migrate",
    "start": "yarn serve",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "ava",
    "coverage-ci": "NODE_ENV=testing nyc --report-dir=../../coverage/service-territory --temp-dir=../../.nyc_output --all --reporter=lcov ava",
    "coverage": "NODE_ENV=testing nyc --all --reporter=text ava",
    "lint": "eslint 'src/**/*.ts'"
  },
  "main": "./dist/bootstrap.js",
  "config": {
    "workingDir": "./dist",
    "bootstrap": "./bootstrap.js",
    "app": {}
  },
  "nyc": {
    "exclude": [
      "src/shared/**",
      "src/**/*.spec.ts",
      "dist/**/*.d.ts"
    ]
  },
  "dependencies": {
    "@ilos/cli": "~0",
    "@ilos/common": "~0",
    "@ilos/connection-manager": "~0",
    "@ilos/connection-postgres": "~0",
    "@ilos/core": "~0",
    "@ilos/framework": "~0",
    "@pdc/helper-test": "~0",
    "@pdc/provider-middleware": "~0",
    "@pdc/provider-validator": "~0"
  }
