{
  "name": "@pdc/service-normalization",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "cli": "ilos",
    "migrate": "echo 'no migrations'",
    "start": "yarn serve",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "ava",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'"
  },
  "main": "./dist/bootstrap.js",
  "config": {
    "workingDir": "./dist",
    "bootstrap": "./bootstrap.js",
    "app": {}
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/interfaces/**",
      "src/types/**",
      "src/exceptions/**",
      "src/config/**"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@ilos/cli": "~0",
    "@ilos/common": "~0",
    "@ilos/connection-postgres": "~0",
    "@ilos/connection-redis": "~0",
    "@ilos/core": "~0",
    "@pdc/helper-test": "~0",
    "@pdc/provider-geo": "~0",
    "@pdc/provider-middleware": "~0",
    "lodash": "^4.17.21"
  }
}
