{
  "name": "@pdc/service-trip",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "cli": "ilos",
    "migrate": "echo 'no migrations'",
    "start": "yarn serve",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "ava",
    "lint": "eslint 'src/**/*.ts'",
    "ilos": "ilos"
  },
  "main": "./dist/bootstrap.js",
  "config": {
    "workingDir": "./dist",
    "bootstrap": "./bootstrap.js",
    "app": {}
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/interfaces/**",
      "src/types/**",
      "src/exceptions/**",
      "src/config/**"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@ilos/cli": "~0",
    "@ilos/common": "~0",
    "@ilos/connection-manager": "~0",
    "@ilos/connection-postgres": "~0",
    "@ilos/connection-redis": "~0",
    "@ilos/core": "~0",
    "@pdc/helper-test": "~0",
    "@pdc/provider-file": "~0",
    "@pdc/provider-middleware": "~0",
    "@pdc/provider-validator": "~0",
    "adm-zip": "^0.5.3",
    "csv-stringify": "^5.6.2",
    "date-fns-tz": "^1.1.2",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  }
}
